{
  "root": true,
  "env": {
      "node": true
  },
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020
  },
  "plugins": [],
  "extends": [
      "standard"
  ],
  "rules": {
    "space-before-function-paren": ["error", "never"],
    "no-unused-vars": ["warn", { "vars": "all", "args": "none", "ignoreRestSiblings": true }],
    "no-async-promise-executor": "warn"
  }
}